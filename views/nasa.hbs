    <img src="https://api.nasa.gov/images/getting_started_use" alt="Image2">
    <button id="myButton" class="float-left submit-button" >Nasa</button>
    <script type="text/javascript">
        document.getElementById("myButton").onclick = function () {
            location.href = "http://jsonplaceholder.typicode.com/photos";
        };
    </script>

</body>
